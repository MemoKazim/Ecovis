<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="refresh" content="3;url=/eng/index" />
    <title>404</title>
  </head>

  <body>
    <h1>Requested URL not found!</h1>
    <p>You'll be redirected in <span id="seconds"> 3 </span> seconds...</p>
  </body>
  <script>
    let timeleft = 1;
    var timer = setInterval(() => {
      if (timeleft >= 3) {
        clearInterval(timer);
      }
      document.getElementById("seconds").innerHTML = 3 - timeleft;
      timeleft += 1;
    }, 1000);
  </script>
</html>
